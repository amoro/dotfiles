# Completions
brew tap homebrew/completions
brew install rails-completion rake-completion bundler-completion gem-completion pip-completion ruby-completion vagrant-completion tmuxinator-completion

# git
brew install git git-flow-avh

# Tools
brew install wget coreutils md5sha1sum ack rename tree cheat
brew install findutils homebrew/dupes/grep --with-default-names

# Command line enhancement
brew install liquidprompt autojump tmux

